<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="WebProLific Suite" thread-count="1" parallel="classes">
    <listeners>
        <listener class-name="listeners.TestListener"/>
        <listener class-name="listeners.AnnotationTransformer"/>
        <listener class-name="listeners.RetryListener"/>
    </listeners>

    <parameter name="browser" value="chrome"/>
    <parameter name="os" value="Windows"/>
    <parameter name="mode" value="UI"/>

    <parameter name="environment" value="new"/>
    <parameter name="client" value="prologic"/>
    <parameter name="plantype" value="All"/>
    <parameter name="implicitwaitduration" value="3"/>
    <parameter name="timeunit" value="seconds"/>
    <parameter name="url" value="https://qc2web.prologicfirst.in/WISH/Common/frmLogin.aspx"/>


    <test name="SRV Test" enabled="false">
        <classes>
            <class name="SRVTest.AdditionalCharge.SRVCharge"/>
            <class name="SRVTest.AdditionalCharge.ItemCharge"/>
            <class name="SRVTest.CashReceipt.CashReceiptTest"/>
            <class name="SRVTest.EditQty.EditTest"/>
            <class name="SRVTest.HeaderLevelCharge.HeaderLevelCharge"/>
            <class name="SRVTest.InterStateVendor.InterStateVendor"/>
            <class name="SRVTest.VendorReturn.VendorReturn"/>
            <class name="SRVTest.WithDirectIssue.SRVPOTest"/>
            <class name="SRVTest.WithDirectIssue.VRTest"/>
            <class name="SRVTest.WithDirectIssue.AddTax"/>
            <class name="SRVTest.WithDirectIssue.EditSRV"/>
            <class name="SRVTest.WithDirectIssueMultiDept.MultiDept"/>
            <class name="SRVTest.WithoutPO.VRTest"/>
            <class name="SRVTest.WithoutPO.AddTax"/>
            <class name="SRVTest.WithoutPO.EditSRV"/>
            <class name="SRVTest.WithPO.AddTax"/>
            <class name="SRVTest.WithPO.EditSRV"/>
            <class name="SRVTest.WithPO.VRTest"/>
        </classes>


    </test>

    <test name="testing" enabled="true">
        <classes>
            <class name="SRVTest.WithPO.VRTest"/>
        </classes>
    </test>


</suite>