<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Edit Requisition" thread-count="1"
	parallel="methods">
	<listeners>
		<listener class-name="listeners.TestListener" />
		<listener class-name="listeners.AnnotationTransformer" />
	</listeners>
	<test name="Prologic Regression Suite" enabled="false">
		<parameter name="browser" value="chrome" />
		<parameter name="os" value="Windows" />
		<parameter name="mode" value="UI" />
		<parameter name="environment" value="prod" />
		<parameter name="client" value="prologic" />
		<parameter name="plantype" value="All" />
		<parameter name="implicitwaitduration" value="10" />
		<parameter name="timeunit" value="seconds" />
		<parameter name="url"
			value="https://qc2web.prologicfirst.in/WISH/Common/frmLogin.aspx" />

		<classes>
			<class name="approveReq_Test.EditReq_Test"></class>
		</classes>
		
	</test>
	
