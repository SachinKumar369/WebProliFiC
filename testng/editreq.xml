		<parameter name="browser" value="chrome" />
		<parameter name="os" value="Windows" />
		<parameter name="mode" value="UI" />
		<parameter name="environment" value="prod" />
		<parameter name="client" value="prologic" />
		<parameter name="plantype" value="All" />
		<parameter name="implicitwaitduration" value="10" />
		<parameter name="timeunit" value="seconds" />
		<parameter name="url"
			value="https://qc2web.prologicfirst.in/WISH/Common/frmLogin.aspx" />

		<classes>
			<class name="editReq.Test.PQEdit_Test"></class>
		</classes>
		
	</test>







</suite>
